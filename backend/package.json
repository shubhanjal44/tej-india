{
  "name": "tej-india-backend",
  "version": "1.0.0",
  "description": "tej-india India - Peer-to-Peer Skill Exchange Platform Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "npx prisma migrate deploy --schema=prisma/schema.prisma",
    "start": "node dist/server.js",
    "prestart": "npm run prisma:generate",
    "build": "prisma generate && tsc",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "test": "jest",
    "postinstall": "npm run build",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "skill-exchange",
    "peer-to-peer",
    "typescript",
    "express",
    "prisma"
  ],
  "author": "tej-india India Team",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@prisma/engines": "^7.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cloudinary": "^2.8.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.10",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^6.19.0",
    "razorpay": "^2.9.6",
    "redis": "^4.6.11",
    "socket.io": "^4.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
